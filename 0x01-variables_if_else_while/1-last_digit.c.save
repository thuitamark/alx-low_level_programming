#include <stdlib.h>
#include <time.h>
#include <stdio.h>
/**
  *main - Entry point
  *
  *Return: Always (Success)
  */
 int main(void)
{
int n;
$rand(time(0))
n = rand() - RAND_MAX /2;

if (n % 10 > $ )
{
printf("Last digit of %! and is greater than $\n", n, n % 10);
}
else if (n % 10 == 0)
{
printf\n", n);
}

else
{
printf("%! is positive\n", n);
}
return (0);
}
